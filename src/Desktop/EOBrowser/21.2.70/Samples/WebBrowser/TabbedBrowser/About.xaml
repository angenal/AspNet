<Window x:Class="EO.TabbedBrowser.About"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="About" Height="200" Width="300" Loaded="Window_Loaded" ShowInTaskbar="False" ResizeMode="NoResize" WindowStartupLocation="CenterOwner">
    <DockPanel>
        <Button Width="80" DockPanel.Dock="Bottom" Height="22" Margin="0,0,0,8" VerticalAlignment="Bottom" Click="Button_Click">OK</Button>
        <StackPanel Orientation="Vertical">
            <TextBlock Margin="0,20,0,0" HorizontalAlignment="Center">EO.WebBrowser Tabbed Browser Demo</TextBlock>
            <TextBlock x:Name="lblVersion" Margin="0,12,0,0" HorizontalAlignment="Center">V x.x.x.x</TextBlock>
            <TextBlock x:Name="lblMoreInfo" Margin="0,12,0,0" HorizontalAlignment="Center" TextWrapping="Wrap"></TextBlock>
        </StackPanel>
    </DockPanel>
</Window>
